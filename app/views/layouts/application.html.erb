<!DOCTYPE html>
<html>
  <head>
    <title>CakeEc</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <link rel="stylesheet" href="assets/top.scss">

    <script src="https://kit.fontawesome.com/f4a34d9917.js" crossorigin="anonymous"></script>
  </head>

  <header class="navbar navbar-fixed-top navbar-default">
  <div class="container">
    <nav class="nav navbar-nav navbar-left">
    </nav>

    <nav>

      <ul class="nav navbar-nav navbar-right">
        <%if user_signed_in? %>

           <li><%= link_to " Logout", destroy_user_session_path, method: :delete, class: "glyphicon glyphicon-log-out" %> </li>

        <% else %>
           <li><%= link_to ' About us', homes_about_path, class: "glyphicon glyphicon-link about" %></li>
           <li ><%= link_to ' Sign up', new_user_registration_path, class: "glyphicon glyphicon-check" %></li>
           <li><%= link_to ' Login', new_user_session_path, class: "glyphicon glyphicon-log-in"%></li>
        <% end %>

      </ul>
    </nav>
  </div>
</header>
  <body>
    <%= yield %>
  </body>
</html>
